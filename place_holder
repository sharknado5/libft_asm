    mov rdi, 1          ;   tut1
    mov rsi, msg
    mov rdx, len
    mov rax, 1
    syscall
    mov rdi, 0
    mov rax, 60
    syscall

    mov rdx, len        ;   tutorialspoint
    mov rsi, msg
    mov rbx, 1
    mov rax, 4
    int 0x80
    mov rax, 1
    int 0x80

    arguments are passed on the registers rdi, rsi, rdx, r10, r8 and r9
    syscall number in the rax register
    the call is done via the syscall instruction